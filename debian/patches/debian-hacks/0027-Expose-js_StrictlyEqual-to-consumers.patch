From 06542eb8f4641b3bb2f4eaeb89476628c871cbe2 Mon Sep 17 00:00:00 2001
From: Mike Hommey <glandium@debian.org>
Date: Wed, 1 Jul 2009 07:47:41 +0200
Subject: [PATCH 27/44] Expose js_StrictlyEqual() to consumers

https://bugzilla.mozilla.org/show_bug.cgi?id=491646
---
 js/src/jsapi.cpp |    6 ++++++
 js/src/jsapi.h   |    3 +++
 2 files changed, 9 insertions(+), 0 deletions(-)

diff --git a/js/src/jsapi.cpp b/js/src/jsapi.cpp
index 6600822..17dec41 100644
--- a/js/src/jsapi.cpp
+++ b/js/src/jsapi.cpp
@@ -703,6 +703,12 @@ JS_GetTypeName(JSContext *cx, JSType type)
     return JS_TYPE_STR(type);
 }
 
+JS_PUBLIC_API(JSBool)
+JS_StrictlyEqual(JSContext *cx, jsval v1, jsval v2)
+{
+    return js_StrictlyEqual(cx, v1, v2);
+}
+
 /************************************************************************/
 
 /*
diff --git a/js/src/jsapi.h b/js/src/jsapi.h
index 131b69c..d03b13d 100644
--- a/js/src/jsapi.h
+++ b/js/src/jsapi.h
@@ -447,6 +447,9 @@ JS_TypeOfValue(JSContext *cx, jsval v);
 extern JS_PUBLIC_API(const char *)
 JS_GetTypeName(JSContext *cx, JSType type);
 
+extern JS_PUBLIC_API(JSBool)
+JS_StrictlyEqual(JSContext *cx, jsval v1, jsval v2);
+
 /************************************************************************/
 
 /*
